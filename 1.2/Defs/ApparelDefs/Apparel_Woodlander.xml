		  <layers>
			<li>Middle</li>
		  </layers>
		  <defaultOutfitTags>
			<li>Soldier</li>
		  </defaultOutfitTags>
		  <tags>
			<li>MSC_General</li>
			<li>RoyalTier6</li>
		  </tags>
		</apparel>
	</ThingDef>	
	
